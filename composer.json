{
  "name": "lidercap/framework-db",
  "description": "Componente de conexão com o banco de dados da Liderança",
  "version": "1.0.0",
  "license": "proprietary",
  "type": "component",
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Lidercap\\Component\\Db\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lidercap\\Tests\\Component\\Db\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.5.9",
    "ext-pdo_dblib": "*"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "phpunit/phpunit": "^4.8",
    "phpmd/phpmd": "2.1.*",
    "sebastian/phpcpd": "2.0.*",
    "sebastian/phpdcd": "^1.0",
    "squizlabs/php_codesniffer": "^2.6"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
